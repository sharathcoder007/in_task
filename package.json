{
  "name": "cloud-ide-llm",
  "version": "1.0.0",
  "description": "A simple cloud IDE with LLM integration for web development",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install-all": "npm install && cd backend && npm install && cd frontend && npm install"
  },
  "keywords": [
    "cloud-ide",
    "llm",
    "react",
    "web-development"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
